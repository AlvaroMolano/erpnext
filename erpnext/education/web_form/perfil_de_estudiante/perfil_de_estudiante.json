 "client_script": "// console.log(frappe)\n\nfrappe.web_form.events.init = () => {\n   frappe.web_form.set_value('student_email_id', frappe.user_id)\n  //  frappe.msgprint('hfhkjd.');\n}\n\n// console.log(frappe.web_form)\n\nfrappe.web_form.validate = () => {\nlet validated = true;\n    var terminos = frappe.web_form.get_value('terms_n_conditions')\n    if(terminos == ''){\n        //frappe.msgprint('Por favor, debes aceptar los t\u00e9rminos y condiciones del servicio.');\n        validated = false;\n        frappe.throw('Por favor, debes aceptar los t\u00e9rminos y condiciones del servicio.');\n    }\n    \n    var politica = frappe.web_form.get_value('politica_privacidad')\n    if(politica == ''){\n        //frappe.msgprint('Por favor, debes aceptar la pol\u00edtica de privacidad.');\n        validated = false;\n        frappe.throw('Por favor, debes aceptar la pol\u00edtica de privacidad.');\n    }\n\n    if(validated){\n        \n    }   \n}\n\n",
